<odoo>
  <data>
    <record model="ir.ui.view" id="pokemon_ability_list">
      <field name="name">pokemon.abilities.list</field>
      <field name="model">pokemon.abilities</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
        </tree>
      </field>
    </record>

    <record model="ir.ui.view" id="taller_ability_form">
      <field name="name">pokemon.abilities.form</field>
      <field name="model">pokemon.abilities</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group name="group_top">
              <group name="group_left">       
                <field name="name"/>
              </group>
            </group>
          </sheet>       
        </form>
      </field>
    </record>

    <record model="ir.actions.act_window" id="ability_action_window">
      <field name="name">HABILIDADES</field>
      <field name="res_model">pokemon.abilities</field>
      <field name="view_mode">tree,form</field>


    </record>
  </data>
</odoo>